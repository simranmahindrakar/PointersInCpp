#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int swap(int *x , int*y) 
{
    int t;
    t=*x;
    *x=*y;
    *y=t;
}

int main() 
{
    int a,b;
    printf("Enter 2 numbers");
    cin>>a>>b;
    swap(&a,&b);
    printf(" a = %d \n" , a);
    printf(" b = %d \n" , b);
    return 0;
    
}
